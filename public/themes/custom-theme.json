  "fonts": {
    "sans": "ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
    "serif": "ui-serif, Georgia, Cambria, 'Times New Roman', Times, serif",
    "mono": "ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, 'Liberation Mono', 'Courier New', monospace"
  },
  "radius": {
    "sm": "calc(var(--radius) - 4px)",
    "md": "calc(var(--radius) - 2px)",
    "lg": "var(--radius)",
    "xl": "calc(var(--radius) + 4px)",
    "2xl": "calc(var(--radius) + 8px)",
    "full": "9999px"
  },
  "shadows": {
    "2xs": "0 1px 3px 0px oklch(0 0 0 / 0.05)",
    "xs": "0 1px 3px 0px oklch(0 0 0 / 0.05)",
    "sm": "0 1px 3px 0px oklch(0 0 0 / 0.10), 0 1px 2px -1px oklch(0 0 0 / 0.10)",
    "md": "0 1px 3px 0px oklch(0 0 0 / 0.10), 0 2px 4px -1px oklch(0 0 0 / 0.10)",
    "lg": "0 1px 3px 0px oklch(0 0 0 / 0.10), 0 4px 6px -1px oklch(0 0 0 / 0.10)",
    "xl": "0 1px 3px 0px oklch(0 0 0 / 0.10), 0 8px 10px -1px oklch(0 0 0 / 0.10)",
    "2xl": "0 1px 3px 0px oklch(0 0 0 / 0.25)"
  },
  "spacing": {
    "1": "0.25rem",
    "2": "0.5rem",
    "3": "0.75rem",
    "4": "1rem",
    "5": "1.25rem",
    "6": "1.5rem",
    "7": "1.75rem",
    "8": "2rem",
    "9": "2.25rem",
    "10": "2.5rem",
    "12": "3rem",
    "16": "4rem",
    "20": "5rem",
    "24": "6rem",
    "32": "8rem",
    "40": "10rem",
    "48": "12rem",
    "56": "14rem",
    "64": "16rem"
  },
  "typography": {
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem",
      "6xl": "3.75rem"
    },
    "lineHeight": {
      "3": "0.75rem",
      "4": "1rem",
      "5": "1.25rem",
      "6": "1.5rem",
      "7": "1.75rem",
      "8": "2rem",
      "9": "2.25rem",
      "10": "2.5rem"
    },
    "fontWeight": {
      "light": "300",
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700",
      "extrabold": "800",
      "black": "900"
    },
    "letterSpacing": {
      "tighter": "-0.05em",
      "tight": "-0.025em",
      "normal": "0em",
      "wide": "0.025em",
      "wider": "0.05em",
      "widest": "0.1em"
    }
  },
  "animations": {
    "duration": {
      "75": "75ms",
      "100": "100ms",
      "150": "150ms",
      "200": "200ms",
      "300": "300ms",
      "500": "500ms",
      "700": "700ms",
      "1000": "1000ms"
    },
    "easing": {
      "linear": "linear",
      "in": "cubic-bezier(0.4, 0, 1, 1)",
      "out": "cubic-bezier(0, 0, 0.2, 1)",
      "in-out": "cubic-bezier(0.4, 0, 0.2, 1)",
      "spring": "cubic-bezier(0.175, 0.885, 0.32, 1.275)"
    },
    "keyframes": {
      "fade-in": {
        "0%": { "opacity": "0", "transform": "translateY(10px)" },
        "100%": { "opacity": "1", "transform": "translateY(0)" }
      },
      "slide-in": {
        "0%": { "opacity": "0", "transform": "translateX(-10px)" },
        "100%": { "opacity": "1", "transform": "translateX(0)" }
      },
      "scale-in": {
        "0%": { "opacity": "0", "transform": "scale(0.95)" },
        "100%": { "opacity": "1", "transform": "scale(1)" }
      },
      "accordion-down": {
        "from": { "height": "0" },
        "to": { "height": "var(--radix-accordion-content-height)" }
      },
      "accordion-up": {
        "from": { "height": "var(--radix-accordion-content-height)" },
        "to": { "height": "0" }
      }
    }
  },
  "semantic": {
    "interactive": {
      "hover": {
        "background": "oklch(0.9245 0.0138 92.9892)",
        "border": "oklch(0.6171 0.1375 39.0427)",
        "shadow": "0 4px 6px -1px oklch(0 0 0 / 0.10), 0 2px 4px -2px oklch(0 0 0 / 0.10)"
      },
      "focus": {
        "ring": "oklch(0.5937 0.1673 253.0630)",
        "ring-offset": "oklch(0.9818 0.0054 95.0986)"
      },
      "active": {
        "background": "oklch(0.9341 0.0153 90.2390)",
        "transform": "scale(0.98)"
      }
    },
    "states": {
      "disabled": {
        "opacity": "0.5",
        "cursor": "not-allowed"
      },
      "loading": {
        "opacity": "0.7",
        "cursor": "wait"
      }
    }
  },
  "components": {
    "button": {
      "variants": {
        "default": {
          "background": "var(--primary)",
          "color": "var(--primary-foreground)",
          "hover": {
            "background": "oklch(from var(--primary) l c h / 0.9)"
          }
        },
        "secondary": {
          "background": "var(--secondary)",
          "color": "var(--secondary-foreground)",
          "hover": {
            "background": "oklch(from var(--secondary) l c h / 0.8)"
          }
        },
        "outline": {
          "border": "1px solid var(--border)",
          "background": "var(--background)",
          "color": "var(--foreground)",
          "hover": {
            "background": "var(--accent)",
            "color": "var(--accent-foreground)"
          }
        },
        "ghost": {
          "background": "transparent",
          "color": "var(--foreground)",
          "hover": {
            "background": "var(--accent)",
            "color": "var(--accent-foreground)"
          }
        },
        "destructive": {
          "background": "var(--destructive)",
          "color": "var(--destructive-foreground)",
          "hover": {
            "background": "oklch(from var(--destructive) l c h / 0.9)"
          }
        }
      },
      "sizes": {
        "sm": {
          "height": "2.25rem",
          "padding": "0.5rem 0.75rem",
          "fontSize": "0.875rem"
        },
        "default": {
          "height": "2.5rem",
          "padding": "0.5rem 1rem",
          "fontSize": "0.875rem"
        },
        "lg": {
          "height": "2.75rem",
          "padding": "0.75rem 2rem",
          "fontSize": "1rem"
        },
        "icon": {
          "height": "2.5rem",
          "width": "2.5rem"
        }
      }
    },
    "card": {
      "background": "var(--card)",
      "border": "1px solid var(--border)",
      "borderRadius": "var(--radius-lg)",
      "shadow": "var(--shadow-sm)",
      "color": "var(--card-foreground)"
    },
    "input": {
      "background": "var(--background)",
      "border": "1px solid var(--border)",
      "borderRadius": "var(--radius-md)",
      "padding": "0.5rem 0.75rem",
      "fontSize": "0.875rem",
      "color": "var(--foreground)",
      "placeholder": {
        "color": "var(--muted-foreground)"
      },
      "focus": {
        "borderColor": "var(--ring)",
        "outline": "none",
        "ring": "2px solid var(--ring)",
        "ringOffset": "2px"
      }
    }
  }
} 